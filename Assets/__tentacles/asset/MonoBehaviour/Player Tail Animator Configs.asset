    slithery: 1
    curling: 1
    springiness: 0
    maxStretching: 0
    wavingSpeed: 1.25
    wavingRange: 1
    wavingAxis:
      x: 1
      y: 0.25
      z: -0.75
  - name: CHASE
    slithery: 0.5
    curling: 0.5
    springiness: 0
    maxStretching: 0
    wavingSpeed: 1.25
    wavingRange: 1
    wavingAxis:
      x: 1
      y: 0.25
      z: -0.75
  - name: ATTACK
